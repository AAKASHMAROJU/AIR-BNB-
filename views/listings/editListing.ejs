<% layout("/layouts/boilerplate.ejs") -%>
<body>
  <h1>Edit Your Listing</h1>
  <form
    action="http://localhost:8080/listings/<%= data._id %>?_method=PATCH"
    method="post"
  >
    <input type="number" name="price" id="" value="<%= data.price %>" />
    <button type="submit">Submit Changes</button>
  </form>
</body>
